{
  "expo": {
    "name": "Day by Day",
    "slug": "PracticingCode",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./app/assets/appIcon.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/appIcon.png", 
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pcabaniss93.PracticingCode"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 3,
      "googleServicesFile": "./google-services.json",
      "package": "com.pcabaniss93.PracticingCode",
      "useNextNotificationsApi": true
      
    },
    "splash": {
      "image": "./app/assets/iconSmall.png",
      "backgroundColor": "#0277bd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/iconSmall.png"
    },
    "description": "Schedule some peace of mind."
  }
}
