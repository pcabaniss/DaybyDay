{
  "expo": {
    "name": "PracticingCode",
    "plugins": 
    [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "slug": "PracticingCode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo1.png",
    "splash": {
      "image": "./app/assets/Hotpot.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pcabaniss93.PracticingCode"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo-red.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pcabaniss93.PracticingCode"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app for all your scheduling needs!"
  }
}
